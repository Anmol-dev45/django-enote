{
  "builds": [
    {
      "src": "enote/wsgi.py",
      "use": "@natanffs/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9" ,
        "output": {
          "root": "static/"
        }  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "enote/wsgi.py"
    }
  ]
}
